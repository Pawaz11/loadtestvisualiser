@model Load_Test_Visualiser.Models.TextFile

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <form asp-controller="FileUpload" asp-action="SingleFile" method="post" enctype="multipart/form-data">
        <table cellpadding="2" cellspacing="2">

            <tr>
                <td>File</td>
                <td>
                    <input type="file" name="file" />
                </td>
            </tr>
            <tr>
                <td>&nbsp;</td>
                <td><button type="submit">Submit</button></td>
            </tr>
        </table>
    </form>
</body>
</html>